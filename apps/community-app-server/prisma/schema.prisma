datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model ChatSession {
  createdAt DateTime @default(now())
  id        String   @id @default(cuid())
  message   String?
  updatedAt DateTime @updatedAt
  user      User?    @relation(fields: [userId], references: [id])
  userId    String?
}

model Admin {
  createdAt DateTime @default(now())
  email     String?
  firstName String?
  id        String   @id @default(cuid())
  lastName  String?
  password  String?
  updatedAt DateTime @updatedAt
  username  String?
}

model User {
  chatSessions  ChatSession[]
  createdAt     DateTime      @default(now())
  email         String?       @unique
  firstName     String?
  id            String        @id @default(cuid())
  lastName      String?
  password      String
  roles         Json
  updatedAt     DateTime      @updatedAt
  userEmail     String?
  userFirstName String?
  userLastName  String?
  username      String        @unique
  usernameField String?
  userPassword  String?
}
